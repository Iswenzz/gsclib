{
	"name": "gsclib",
	"version": "2.0.0",
	"description": "gsclib acts as a standard library extension for the Call of Duty 4 scripting language.",
  	"homepage": "https://github.com/Iswenzz/gsclib",
	  "features": {
		"tests": {
			"description": "Build tests",
			"dependencies": ["gtest"]
		}
	},
	"dependencies": [
		"curl",
		"libzippp",
		"mariadb-connector-cpp",
		"pcre2"
	],
	"overrides": [
        { "name": "curl", "version": "8.14.1" },
        { "name": "gtest", "version": "1.17.0#1" },
		{ "name": "libmariadb", "version": "3.3.1#1" },
        { "name": "libzippp", "version": "7.1-1.10.1" },
        { "name": "mariadb-connector-cpp", "version": "1.1.5" },
        { "name": "pcre2", "version": "10.45" }
    ]
}
